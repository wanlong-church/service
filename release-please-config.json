{
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "node",
      "include-component-in-tag": false,
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "pull-request-title-pattern": "release: release ${version}",
      "draft": false,
      "prerelease": false,
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features"
        },
        {
          "type": "fix",
          "section": "Bug Fixes"
        },
        {
          "type": "build",
          "section": "Build Related"
        },
        {
          "type": "chore",
          "section": "Chores"
        },
        {
          "type": "refactor",
          "section": "Refactors"
        },
        {
          "type": "docs",
          "section": "Documentation"
        },
        {
          "type": "test",
          "section": "Tests"
        }
      ]
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
